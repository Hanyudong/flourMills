<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cn.sjzc.flour.pojo.GrainPurchase" table="t_grainpurchase">
	
        <id name="purId" >
            <generator class="native"/>
        </id>
        <property name="evidenceId" unique="true" not-null="true"/> 
        <property name="purDate"/>   
        <property name="purName"/>  
        <property name="purWeight"/>
        <property name="purBag"/>  
        <property name="totalWeight"/>  
        <property name="price"/>
        <property name="totalPrices"/>
    </class>
</hibernate-mapping>