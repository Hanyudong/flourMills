<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cn.sjzc.flour.pojo.FlourProcess" table="t_flourprocess">
	
        <id name="processId" >
            <generator class="native"/>
        </id>
        <many-to-one name="systemUser" column="userId"></many-to-one>
        <property name="typeName"/>
        <property name="stockSum"/>
        <property name="processWeight"/>
        <property name="strageWeight"/>
        <property name="branWeight"/>
        <property name="date"/>
        <property name="evno"/>   
    </class>
</hibernate-mapping>
